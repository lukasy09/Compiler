<?JD

claim ax = 2+3;
claim a = 3;

func add(a, b) {
    return a + b;
}
func factorial(n){

    claim condition1 = n==1;
    claim condition2 = n==0;

    if(condition1 or condition2){
        return 1;
    };
    claim next = n-1;
    return factorial(next) * n;
}
while (a > 5) {
    claim xd = 5;
    a = a + 1;
    ax = factorial(5);
}
claim result = factorial(3);
?JD>